{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MqttSettings": {
    "BrokerHost": "localhost",
    "BrokerPort": 1883,
    "ClientId": "api_gateway_client"
  },
  "ChromaDB": {
    "Uri": "http://localhost:8000/api/v1/",
    "CollectionName": "documents"
  },
  "Redis": {
    "Host": "localhost",
    "Port": 6379,
    "CacheExpirationMinutes": 60
  },
  "VectorService": {
    "UseOllama": false
  }
}
